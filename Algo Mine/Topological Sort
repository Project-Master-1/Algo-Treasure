#include <iostream>
#include <vector>
#include <queue>
using namespace std;

void topologicalSort(int V, vector<int> adj[]) {
    vector<int> indegree(V, 0);

    for (int u = 0; u < V; u++)
        for (int v : adj[u])
            indegree[v]++;

    queue<int> q;
    for (int i = 0; i < V; i++)
        if (indegree[i] == 0) q.push(i);

    vector<int> topo;
    while (!q.empty()) {
        int u = q.front(); q.pop();
        topo.push_back(u);

        for (int v : adj[u])
            if (--indegree[v] == 0)
                q.push(v);
    }

    cout << "Topological Order: ";
    for (int x : topo) cout << x << " ";
    cout << endl;
}

int main() {
    int V = 6;
    vector<int> adj[V];
    adj[5] = {0, 2};
    adj[4] = {0, 1};
    adj[2] = {3};
    adj[3] = {1};

    topologicalSort(V, adj);
}
